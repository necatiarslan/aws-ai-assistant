{
  "name": "aws-ai-assistant",
  "displayName": "Aws AI Assistant",
  "description": "Aws AI Assistant",
  "version": "1.0.0",
  "publisher": "NecatiARSLAN",
  "repository": {
    "type": "git",
    "url": "https://github.com/necatiarslan/aws-ai-assistant"
  },
  "icon": "media/aws-ai-assistant-logo.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aws-ai-assistant.RefreshCredentials",
        "title": "Aws AI Assistant:Refresh Credentials"
      },
      {
        "command": "aws-ai-assistant.ListAwsProfiles",
        "title": "Aws AI Assistant:List Profiles"
      },
      {
        "command": "aws-ai-assistant.SetAwsProfile",
        "title": "Aws AI Assistant:Set Active Profile"
      },
      {
        "command": "aws-ai-assistant.TestAwsConnectivity",
        "title": "Aws AI Assistant:Test Aws Connectivity With Active User"
      },
      {
        "command": "aws-ai-assistant.SetAwsEndpoint",
        "title": "Aws AI Assistant:Set AWS Endpoint"
      },
      {
        "command": "aws-ai-assistant.SetDefaultRegion",
        "title": "Aws AI Assistant:Set Default AWS Region"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^9.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.0",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "glob": "^13.0.0",
    "mocha": "^11.7.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.948.0",
    "@aws-sdk/client-iam": "^3.948.0",
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/client-sts": "^3.948.0",
    "@aws-sdk/credential-provider-node": "^3.948.0",
    "@aws-sdk/credential-providers": "^3.948.0",
    "@aws-sdk/types": "^3.936.0"
  }
}
